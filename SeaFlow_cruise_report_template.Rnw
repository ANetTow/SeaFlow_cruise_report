	 		 coord_fixed(ratio = 1, xlim = range(this_stat[,'lon'], na.rm=T), ylim = range(this_stat[, 'lat'], na.rm = T)) +
	 		 theme_bw() + annotation_custom(a) +
	 		 scale_color_gradientn(colours=viridis::viridis(100), name = 'Qc')
	 }
	 grid.arrange(grobs = p, nrow = cnt/2, widths = c(1.5, 1))
@
	\caption{Carbon quota (pg C cell$^{-1}$) time series and maps of identified phytoplankton populations (\Sexpr{phyto_inline}).}
   \label{fig:phyP_Qc}
\end{figure}

